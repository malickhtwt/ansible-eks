apiVersion: eksctl.io/v1alpha5
kind: ClusterConfig

metadata:
  name: {{ cluster_name }}
  region: {{ aws_region }}

nodeGroups:
  - name: {{ nodegroup_name }}
    instanceType: {{ instancetype }}
    desiredCapacity: {{ desiredcapacity }}
    volumeSize: {{ volumesize }}
    ssh:
      publicKeyPath: /home/vagrant/eks/ansible-eks.pub
